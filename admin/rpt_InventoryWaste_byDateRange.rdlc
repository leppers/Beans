        <rd:TableAdapterName>InventoryWasteInfoTableAdapter</rd:TableAdapterName>
        <rd:TableAdapterFillMethod>Fill</rd:TableAdapterFillMethod>
        <rd:TableAdapterGetDataMethod>GetInventoryWaste_ByDateRange</rd:TableAdapterGetDataMethod>
      </rd:DataSetInfo>
    </DataSet>
