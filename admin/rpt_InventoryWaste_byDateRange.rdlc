  <DataSets>
    <DataSet Name="Beans_DB_v5DataSet_InventoryWasteInfo">
      <Fields>
        <Field Name="Location">
          <DataField>Location</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Item">
          <DataField>Item</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Date_Of">
          <DataField>Date Of</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Waste__lbs_">
          <DataField>Waste (lbs)</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Beans_DB_v5ConnectionString</DataSourceName>
        <CommandText>SELECT        InvLin.Location_ID AS Location, Inventory_Item.Item_Name AS Item, InvLin.InvLin_Date AS [Date Of], 
                         InvLin.InvLin_Quantity * Unit.Unit_Weight AS [Waste (lbs)]
FROM            InvLin INNER JOIN
                         Unit ON InvLin.Unit_ID = Unit.Unit_ID INNER JOIN
                         Inventory_Item ON Unit.Item_ID = Inventory_Item.Item_ID
WHERE        (InvLin.InvLin_Date &gt;= @startDate) AND (InvLin.InvLin_Date &lt;= @endDate) AND (InvLin.InvLin_Action = 'waste')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <rd:DataSetInfo>
        <rd:DataSetName>Beans_DB_v5DataSet</rd:DataSetName>
