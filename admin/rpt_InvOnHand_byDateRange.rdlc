  <DataSets>
    <DataSet Name="Beans_DB_v5DataSet_PIC_INV_On_Hand">
      <Fields>
        <Field Name="PIC_Date">
          <DataField>PIC_Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Item_Name">
          <DataField>Item_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Location_Name">
          <DataField>Location_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_Lbs">
          <DataField>Total Lbs</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Beans_DB_v5ConnectionString</DataSourceName>
        <CommandText>SELECT        PIC.PIC_Date, Inventory_Item.Item_Name, Location.Location_Name, SUM(Unit.Unit_Weight * PIC_LineItem.PIC_Item_Quantity) AS [Total Lbs]
FROM            PIC INNER JOIN
                         PIC_LineItem ON PIC.PIC_ID = PIC_LineItem.PIC_ID INNER JOIN
                         Location ON PIC_LineItem.Location_ID = Location.Location_ID INNER JOIN
                         Inventory_Item ON PIC_LineItem.PIC_Item_ID = Inventory_Item.Item_ID INNER JOIN
                         Unit ON PIC_LineItem.Unit_ID = Unit.Unit_ID AND Inventory_Item.Item_ID = Unit.Item_ID
WHERE        (PIC.PIC_Date =
                             (SELECT        MAX(PIC_Date) AS greatestDate
                               FROM            PIC AS PIC_1))
GROUP BY Inventory_Item.Item_Name, Location.Location_Name, PIC.PIC_Date, PIC_LineItem.PIC_Item_Quantity</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
