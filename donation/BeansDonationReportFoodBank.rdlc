      <Fields>
        <Field Name="Donation_ID">
          <DataField>Donation_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Donation_DateTime">
          <DataField>Donation_DateTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Entity_Organization_Name">
          <DataField>Entity_Organization_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalWeight">
          <DataField>TotalWeight</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Beans_DB_v4ConnectionString</DataSourceName>
        <CommandText>SELECT DISTINCT
D.Donation_ID, D.Donation_DateTime, E.Entity_Organization_Name, SUM(DL.DonLin_Weight) AS TotalWeight
FROM
Donation D, Entity E, DonLin DL
WHERE
D.Donation_ID=DL.Donation_ID And E.Entity_ID=D.Entity_ID And DL.Category_ID&lt;&gt;16 And D.Donation_Action Like 'In' and D.Entity_ID=10
GROUP BY D.Donation_ID, D.Donation_DateTime, E.Entity_Organization_Name
ORDER BY D.Donation_ID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
