        </Field>
        <Field Name="Weight">
          <DataField>Weight</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="category_name">
          <DataField>category_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Beans_DB_v4ConnectionString</DataSourceName>
        <CommandText>Select d.Donation_ID, e.Entity_Organization_Name, d.Donation_datetime, c.category_name, dl.Item_description, SUM(DL.DonLin_Weight) as Weight
From Donation d, DonLin DL, Entity E, Category c
Where Donation_Action like 'waste' and d.Donation_ID=DL.Donation_ID and D.Entity_ID=E.Entity_ID and DL.Category_ID=c.Category_ID
Group by d.Donation_ID, E.Entity_Organization_Name, d.Donation_DateTime, c.Category_Name,dl.Item_Description</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
